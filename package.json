{
  "name": "ts-express-starter",
  "version": "0.1.1",
  "description": "A minimal starter template to quickly get started with Express projects using TypeScript",
  "main": "src/index.ts",
  "scripts": {
    "prod": "tsc && node ./build/index.js",
    "dev": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "format": "prettier --check src/index.ts"
  },
  "keywords": [
    "TypeScript",
    "Express",
    "Starter"
  ],
  "author": "Usman Najeeb",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  }
}
